{
  "name": "vue-antd-template",
  "description": "vue SPA + antdv + vite template",
  "version": "0.0.2",
  "private": true,
  "author": "aaronjxz@gmail.com",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "build": "vite build --config src/apps/vite.config.js",
    "local": "vite --config src/apps/vite.config.js",
    "local:reload": "vite --force --config src/apps/vite.config.js",
    "local:mocked": "vite --config src/apps/vite.config.js --mode mocked",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "prettier:check": "prettier --check **/*.{js,json,vue}",
    "prettier:write": "prettier --write **/*.{js,json,vue}",
    "test:e2e": "echo \"Remember to run frontend and backend before test...\" && playwright test"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@es-labs/esm": "^0.0.24",
    "@sentry/vue": "^8.34.0",
    "ant-design-vue": "^4.2.5",
    "pinia": "^2.2.4",
    "vue": "^3.3.8",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.0",
    "msw": "^2.4.11",
    "prettier": "^3.3.3",
    "vite": "^5.4.9"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
